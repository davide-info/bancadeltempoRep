<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping package="entita">
    <class name="entita.Utente" entity-name="utente" >
        <id name="indirizzoEmail" column="indirizzo_email" type="string"></id>    
        <property name="nome" column="nome" type="string"/>
        <property name="cognome" column="cognome" type="string"/>
        <property name="password" column="password" type="string"/>
        <property name="saldoIniziale" column="saldo_iniziale" type="int"/>
        <property name="zona" column="fk_zona" type="int"/>
        
        
            
            
        
        
    </class>


</hibernate-mapping>
