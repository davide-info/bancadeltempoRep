<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping>
    <class name="entita.Telefono" entity-name="telefono">
        <id name="idTelefono" type="int" column="id_telefono">
        </id>
        <property name="numeroTelefono" type="string" column="numero_telefono"/>
        <property name="utente" type="string" column="fk_utente">
        </property>
    </class>
</hibernate-mapping>
